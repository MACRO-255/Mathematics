   "source": [
    "# Define the function we are going to integrate\n",
    "f=cos(x)+(sin(x))^4\n",
    "\n",
    "domainF_Start=0\n",
    "domainF_End=1.5\n",
    "domainF_Size=domainF_End-domainF_Start\n",
    "\n",
    "plot(f,x,domainF_Start,domainF_End)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 67,
   "metadata": {},
   "outputs": [
